{
  "name": "ara-content-manager",
  "productName": "Ara Content Manager",
  "version": "1.0.0",
  "description": "",
  "main": "boot/main.js",
  "scripts": {
    "start-dev": "electron boot/main.js",
    "test": "electron test/main.js",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littlstar/ara-content-manager.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/littlstar/ara-content-manager/issues"
  },
  "homepage": "https://github.com/littlstar/ara-content-manager#readme",
  "dependencies": {
    "ara-context": "github:arablocks/ara-context",
    "ara-contracts": "../ara_blocks/ara-contracts",
    "ara-crypto": "github:arablocks/ara-crypto",
    "ara-filesystem": "github:arablocks/ara-filesystem",
    "ara-identity": "github:arablocks/ara-identity",
    "ara-network": "github:arablocks/ara-network",
    "ara-network-node-dcdn-farm": "github:littlstar/ara-network-node-dcdn-farm",
    "ara-util": "github:arablocks/ara-util",
    "body-parser": "^1.18.3",
    "choo": "^6.13.0",
    "cors": "^2.8.4",
    "css": "file:browser/lib/cssTool",
    "domready": "^1.0.8",
    "electron-is-dev": "^0.3.0",
    "electron-positioner": "^3.0.1",
    "electron-window-manager": "^1.0.6",
    "filesize": "^3.6.1",
    "md5-hex": "^2.0.0",
    "menubar": "*",
    "nanocomponent": "^6.5.2",
    "styleUtils": "file:browser/lib/styleUtils",
    "typedarray-to-buffer": "^3.1.5",
    "user-home": "^2.0.0",
    "web3-providers-ws": "^1.0.0-beta.35"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "chai": "^4.1.2",
    "debug": "^3.1.0",
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "electron-rebuild": "^1.7.3",
    "electron-reload": "^1.2.3",
    "mocha": "^5.2.0"
  },
  "build": {
    "appId": "com.littlstar.ara-content-manager",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "protocols": {
      "name": "Ara Content Manager",
      "schemes": [
        "lstr"
      ]
    },
    "win": {}
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "sources": [
      "src/*.{js}"
    ]
  }
}
