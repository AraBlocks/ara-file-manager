{
  "name": "ara-file-manager",
  "productName": "Ara File Manager",
  "description": "Ara file manager",
  "author": {
    "name": "ara blocks"
  },
  "version": "0.11.0",
  "main": "boot",
  "network": "test",
  "scripts": {
    "start-dev": "./node_modules/.bin/electron boot",
    "test": "electron test/main.js",
    "postinstall": "sh ./scripts/postinstall.sh && install-app-deps",
    "pack": "build --dir",
    "mac-installer": "sh ./scripts/mac-installer.sh",
    "build-dev-mac": "sh ./scripts/mac-dev-build.sh",
    "build-dev-windows": "sh ./scripts/windows-dev-build.sh",
    "build-prod-mac": "sh ./scripts/mac-production-build.sh",
    "build-prod-windows": "sh ./scripts/windows-production-build.sh",
    "debug-mac": "DEBUG=afm*,arp*,ard* release-builds/Ara\\ File\\ Manager-darwin-x64/Ara\\ File\\ Manager.app/Contents/MacOS/Ara\\ File\\ Manager",
    "debug-windows": "DEBUG=afm*,arp*,ard* release-builds/Ara\\ File\\ Manager-win32-x64/Ara\\ File\\ Manager.exe",
    "changelog": "conventional-changelog --same-file --preset angular --release-count 0 --infile CHANGELOG.md",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/littlstar/ara-content-manager.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/littlstar/ara-content-manager/issues"
  },
  "homepage": "https://github.com/littlstar/ara-content-manager#readme",
  "dependencies": {
    "ara-context": "github:arablocks/ara-context#semver:0.5.x",
    "ara-contracts": "github:arablocks/ara-contracts#semver:0.23.x",
    "ara-filesystem": "github:arablocks/ara-filesystem#semver:0.25.x",
    "ara-identity": "github:arablocks/ara-identity#semver:0.47.x",
    "ara-reward-dcdn": "github:arablocks/ara-reward-dcdn#semver:0.19.x",
    "ara-runtime-configuration": "github:arablocks/ara-runtime-configuration#semver:1.1.x",
    "ara-util": "github:arablocks/ara-util#semver:1.3.x",
    "component-box": "github:MarkGeeRomano/component-box#mgr-constructor-args",
    "css": "file:browser/lib/cssTool",
    "did-uri": "^0.2.6",
    "domready": "^1.0.8",
    "electron-is-dev": "^0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-manager": "^1.0.6",
    "filesize": "^3.6.1",
    "k": "file:lib/constants",
    "md5-hex": "^2.0.0",
    "mirror-folder": "^3.0.0",
    "nanocomponent": "^6.5.2",
    "nanohtml": "^1.4.0",
    "pify": "^4.0.1",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "styleUtils": "file:browser/lib/styleUtils",
    "toiletdb": "^1.4.1",
    "truffle": "^4.1.14",
    "universal-analytics": "^0.4.20",
    "user-home": "^2.0.0",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^2.0.11",
    "debug": "^3.1.0",
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-windows": "^1.1.0",
    "electron-rebuild": "^1.7.3",
    "electron-reload": "^1.2.3",
    "electron-winstaller": "^2.7.0",
    "mocha": "^5.2.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "sources": [
      "src/*.{js}"
    ]
  }
}
